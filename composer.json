{
    "name": "innoboxrr/laravel-auth",
    "description": "Setup inicial para el proceso de autenticaci√≥n con laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Innoboxrr\\LaravelAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Innoboxrr\\LaravelAuth\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Homero Raul",
            "email": "h.raulvc@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "orchestra/testbench": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Innoboxrr\\LaravelAuth\\Providers\\LaravelAuthServiceProvider",
                "Innoboxrr\\LaravelAuth\\Providers\\RouteServiceProvider",
                "Innoboxrr\\LaravelAuth\\Providers\\EmailVerificationServiceProvider",
                "Innoboxrr\\LaravelAuth\\Providers\\ResetPasswordServiceProvider"
            ]
        }
    },
    "require": {
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16"
    }
}